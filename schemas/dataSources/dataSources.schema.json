{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "description": "Data sources to load the data required to fill the report's fields.",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "id": {
        "type": "string"
      },
      "guid": {
        "type": "string"
      },
      "type": {
        "type": "string",
        "enum": [
          "database",
          "file"
        ]
      }
    },
    "required": [
      "id",
      "guid",
      "type"
    ]
  }
}
